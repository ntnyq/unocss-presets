exports[`custom prefix > should match snapshot 1`] = `
"/* layer: preflights */
:root {
--s-star-clip-path: polygon(50% 0,79% 90%,2% 35%,98% 35%,21% 90%);
--s-triangle-clip-path: polygon(50% 0,100% 100%,0 100%);
--s-rhombus-clip-path: polygon(50% 0,100% 50%,50% 100%,0 50%);
--s-hexagon-clip-path: polygon(50% -50%,100% 50%,50% 150%,0 50%);
--s-octagon-angle: calc(50% * tan(-22.5deg));
--s-octagon-clip-path: polygon(var(--s-octagon-angle) 50%, 50% var(--s-octagon-angle), calc(100% - var(--s-octagon-angle)) 50%, 50% calc(100% - var(--s-octagon-angle)));
--s-heart-gradient-mask: radial-gradient(#000 69%,#0000 70%) 84.5%/50%;
--s-heart-clip-path: polygon(-41% 0,50% 91%, 141% 0);
--s-parallelogram-skew: 30%;
--s-parallelogram-clip-path: polygon(var(--s-parallelogram-skew) 0,100% 0,calc(100% - var(--s-parallelogram-skew)) 100%,0 100%);
}
/* layer: default */
.s-star-100px{width:100px;aspect-ratio:1;clip-path:var(--s-star-clip-path);}
.s-triangle-100px{width:100px;aspect-ratio:1/cos(30deg);clip-path:var(--s-triangle-clip-path);}
.s-rhombus-100px{height:100px;aspect-ratio:1;clip-path:var(--s-rhombus-clip-path);}
.s-hexagon-100px{height:100px;aspect-ratio:1/cos(30deg);clip-path:var(--s-hexagon-clip-path);}
.s-octagon-100px{width:100px;aspect-ratio:1;clip-path:var(--s-octagon-clip-path);}
.s-heart-100px{height:100px;aspect-ratio:1;-webkit-mask-box-image:var(--s-heart-gradient-mask);mask-border:var(--s-heart-gradient-mask);clip-path:var(--s-heart-clip-path);}
.s-parallelogram-100px{height:100px;aspect-ratio:3/2;clip-path:var(--s-parallelogram-clip-path);}"
`;

