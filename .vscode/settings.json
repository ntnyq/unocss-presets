{
  "eslint.enable": true,
  "prettier.enable": true,
  "editor.formatOnSave": true,
  "prettier.configPath": "./prettier.config.mjs",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit",
    "source.organizeImports": "never",
    "source.sortImports": "never"
  },
  "eslint.validate": [
    "vue",
    "yaml",
    "toml",
    "json",
    "jsonc",
    "json5",
    "markdown",
    "javascript",
    "typescript",
    "javascriptreact",
    "typescriptreact"
  ],
  "cSpell.words": [
    "attributify",
    "bezier",
    "bumpp",
    "changelogithub",
    "crema",
    "easings",
    "ginza",
    "hefe",
    "iconify",
    "kekab",
    "lcov",
    "lofi",
    "nocheck",
    "perpetua",
    "pnpm",
    "stinson",
    "sutro",
    "tabler",
    "tada",
    "unplugin",
    "vite",
    "vitest",
    "vueuse",
    "xingtong",
    "xpro"
  ],
  "cSpell.allowCompoundWords": true
}
